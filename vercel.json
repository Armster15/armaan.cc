{
  "trailingSlash": false,
  "redirects": [
    {
      "source": "/projects",
      "destination": "/work",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/bee.js",
      "destination": "https://cdn.splitbee.io/sb.js"
    },
    {
      "source": "/_hive/:slug",
      "destination": "https://hive.splitbee.io/:slug"
    }
  ]
}

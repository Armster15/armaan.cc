{
  "trailingSlash": false,
  "redirects": [
    {
      "source": "/projects",
      "destination": "/work",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/_1ds",
      "destination": "https://collector.onedollarstats.com/events"
    },
    {
      "source": "/_1ds/:slug",
      "destination": "https://collector.onedollarstats.com/events/:slug"
    },
    {
      "source": "/1ds.js",
      "destination": "https://assets.onedollarstats.com/stonks.js"
    }
  ]
}
